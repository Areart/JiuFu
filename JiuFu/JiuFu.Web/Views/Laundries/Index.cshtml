@model IEnumerable<JiuFu.Entities.Laundry>

@{
    ViewData["Title"] = "Index";
    Layout = "";
}

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>送物</title>
    <link href="~/css/bootstarpcss/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/bootstarpcss/bj.css">
</head>
<body>
    <meta name="viewport" content="width=device-width,initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <div class="container" style="height:100%">
        <div class="row" id="div-header">
            <div class="col-xs-3">
                <a class="btn btn-default" href="#" role="button">返回</a>
            </div>
            <div class="col-xs-6">
                <p style="text-align:center;font-size:20px;">保洁</p>
            </div>

        </div>



        <div class="row">
            @foreach (var item in Model)
            {
                <div class="row" id="main-rigth">


                    <a asp-action="Details" asp-route-id="@item.Id">
                        <div class="col-xs-4">
                            <div class="thumbnail">
                                <img src="~/Images/xiyiji.png" alt="...">
                            </div>
                        </div>
                        <div class="col-xs-1">
                            <h4 id="main-rigth1">洗送衣物</h4>
                        </div>
                    </a>
                </div>



                <div class="row" id="main-rigth">
                    <a asp-action="Details" asp-route-id="@item.Id">
                        <div class="col-xs-4">
                            <div class="thumbnail">
                                <img src="~/Images/dasao.png" alt="...">
                            </div>
                        </div>
                        <div class="col-xs-1">
                            <h4 id="main-rigth1">打扫房间</h4>
                        </div>
                    </a>
                </div>

            }



        </div>

    </div>
</body>
